    .thread T_0;
    .scope S_0x5704b4f68620;
T_1 ;
    %wait E_0x5704b4fa42e0;
    %load/vec4 v0x5704b4fcb9a0_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %pushi/vec4 0, 0, 2;
    %assign/vec4 v0x5704b4fcb800_0, 0;
    %pushi/vec4 0, 0, 3;
    %assign/vec4 v0x5704b4f9fe30_0, 0;
    %pushi/vec4 0, 0, 8;
    %assign/vec4 v0x5704b4fcbd60_0, 0;
    %pushi/vec4 0, 0, 8;
    %assign/vec4 v0x5704b4fcb660_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x5704b4fcb740_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x5704b4fcb800_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 2;
    %cmp/u;
    %jmp/1 T_1.2, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 2;
    %cmp/u;
    %jmp/1 T_1.3, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 2;
    %cmp/u;
    %jmp/1 T_1.4, 6;
    %jmp T_1.5;
T_1.2 ;
    %pushi/vec4 0, 0, 3;
    %assign/vec4 v0x5704b4f9fe30_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x5704b4fcb740_0, 0;
    %load/vec4 v0x5704b4fcb5a0_0;
    %cmpi/e 0, 0, 1;
    %jmp/0xz  T_1.6, 4;
    %pushi/vec4 1, 0, 2;
    %assign/vec4 v0x5704b4fcb800_0, 0;
T_1.6 ;
    %jmp T_1.5;
T_1.3 ;
    %load/vec4 v0x5704b4fcb5a0_0;
    %cmpi/e 1, 0, 1;
    %jmp/0xz  T_1.8, 4;
    %pushi/vec4 0, 0, 2;
    %assign/vec4 v0x5704b4fcb800_0, 0;
    %jmp T_1.9;
T_1.8 ;
    %load/vec4 v0x5704b4fcbca0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.10, 8;
    %load/vec4 v0x5704b4fcbd60_0;
    %parti/s 7, 0, 2;
    %load/vec4 v0x5704b4fcb8e0_0;
    %concat/vec4; draw_concat_vec4
    %assign/vec4 v0x5704b4fcbd60_0, 0;
    %load/vec4 v0x5704b4f9fe30_0;
    %addi 1, 0, 3;
    %assign/vec4 v0x5704b4f9fe30_0, 0;
    %load/vec4 v0x5704b4f9fe30_0;
    %cmpi/e 7, 0, 3;
    %jmp/0xz  T_1.12, 4;
    %pushi/vec4 2, 0, 2;
    %assign/vec4 v0x5704b4fcb800_0, 0;
T_1.12 ;
T_1.10 ;
T_1.9 ;

v0x5704b4fcb9a0_0 .net "rst", 0 0, o0x7d5fd96e2198;  0 drivers
o0x7d5fd96e21c8 .functor BUFZ 1, C4<z>; HiZ drive
v0x5704b4fcba60_0 .net "sck", 0 0, o0x7d5fd96e21c8;  0 drivers
v0x5704b4fcbb20_0 .var "sck_antigo", 0 0;
v0x5704b4fcbbe0_0 .var "sck_atual", 0 0;
v0x5704b4fcbca0_0 .net "sck_rise", 0 0, L_0x5704b4fa40c0;  1 drivers
v0x5704b4fcbd60_0 .var "shift_reg", 7 0;
E_0x5704b4fa42e0/0 .event negedge, v0x5704b4fcb9a0_0;
E_0x5704b4fa42e0/1 .event posedge, v0x5704b4fcb500_0;
E_0x5704b4fa42e0 .event/or E_0x5704b4fa42e0/0, E_0x5704b4fa42e0/1;
    .scope S_0x5704b4f68620;
T_0 ;
    %wait E_0x5704b4fa42e0;
    %load/vec4 v0x5704b4fcb9a0_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x5704b4fcbbe0_0, 0;
    %pushi/vec4 0, 0, 1;

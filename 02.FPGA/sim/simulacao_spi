 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst";
    .port_info 2 /INPUT 1 "sck";
    .port_info 3 /INPUT 1 "mosi";
    .port_info 4 /INPUT 1 "cs";
    .port_info 5 /OUTPUT 8 "data_out";
    .port_info 6 /OUTPUT 1 "data_valid";
    .port_info 7 /OUTPUT 1 "sck_rise";
enum0x60644ca7c890 .enum4 (2)
   "IDLE" 2'b00,
   "RECEIVE" 2'b01,
   "DONE" 2'b10
 ;
L_0x60644caa0a50 .functor NOT 1, v0x60644cac57e0_0, C4<0>, C4<0>, C4<0>;
L_0x60644ca930c0 .functor AND 1, v0x60644cac58a0_0, L_0x60644caa0a50, C4<1>, C4<1>;

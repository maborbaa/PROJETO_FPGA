    %store/vec4 v0x60644cac6370_0, 0, 1;
    %delay 100000, 0;
    %vpi_call/w 3 53 "$display", "Iniciando transmissao SPI..." {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x60644cac6080_0, 0, 1;
    %delay 200000, 0;
    %pushi/vec4 165, 0, 8;
    %store/vec4 v0x60644cac5e90_0, 0, 8;
    %fork TD_tb_spi_slave.enviar_byte, S_0x60644cac5c00;
    %join;
    %delay 100000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x60644cac6080_0, 0, 1;
    %delay 200000, 0;
    %vpi_call/w 3 66 "$display", "Teste finalizado. Verifique se data_out = A5 e data_valid pulsou." {0 0 0};
    %vpi_call/w 3 67 "$finish" {0 0 0};
    %end;
    .thread T_4;

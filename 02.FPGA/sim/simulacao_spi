v0x60644cac5a20_0 .var "shift_reg", 7 0;
E_0x60644ca93b80/0 .event negedge, v0x60644cac51b0_0;
E_0x60644ca93b80/1 .event posedge, v0x60644cac51b0_0;
E_0x60644ca93b80 .event/or E_0x60644ca93b80/0, E_0x60644ca93b80/1;
S_0x60644cac5c00 .scope task, "enviar_byte" "enviar_byte" 3 71, 3 71 0, S_0x60644ca98870;
 .timescale -9 -12;
v0x60644cac5db0_0 .var/i "i", 31 0;
v0x60644cac5e90_0 .var "valor", 7 0;
TD_tb_spi_slave.enviar_byte ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x60644cac5db0_0, 0, 32;
T_0.0 ;
    %load/vec4 v0x60644cac5db0_0;
    %cmpi/s 7, 0, 32;
    %flag_or 5, 4;
    %jmp/0xz T_0.1, 5;
    %load/vec4 v0x60644cac5e90_0;
    %pushi/vec4 7, 0, 32;
    %load/vec4 v0x60644cac5db0_0;
    %sub;
    %part/s 1;
    %store/vec4 v0x60644cac6280_0, 0, 1;
    %delay 100000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x60644cac6410_0, 0, 1;
    %delay 100000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x60644cac6410_0, 0, 1;
    %delay 100000, 0;
    %load/vec4 v0x60644cac5db0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x60644cac5db0_0, 0, 32;
    %jmp T_0.0;
T_0.1 ;
    %end;
    .scope S_0x60644ca98a00;
T_1 ;
    %wait E_0x60644ca93b80;
    %load/vec4 v0x60644cac56a0_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x60644cac58a0_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x60644cac57e0_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x60644cac5740_0;
    %assign/vec4 v0x60644cac58a0_0, 0;
    %load/vec4 v0x60644cac58a0_0;
    %assign/vec4 v0x60644cac57e0_0, 0;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x60644ca98a00;
T_2 ;
    %wait E_0x60644ca93b80;
    %load/vec4 v0x60644cac56a0_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_2.0, 8;
    %pushi/vec4 0, 0, 2;
    %assign/vec4 v0x60644cac5500_0, 0;
    %pushi/vec4 0, 0, 3;
    %assign/vec4 v0x60644caa0c70_0, 0;
    %pushi/vec4 0, 0, 8;
    %assign/vec4 v0x60644cac5a20_0, 0;
    %pushi/vec4 0, 0, 8;
    %assign/vec4 v0x60644cac5310_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x60644cac5440_0, 0;
    %jmp T_2.1;
T_2.0 ;
    %load/vec4 v0x60644cac5500_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 2;
    %cmp/u;
    %jmp/1 T_2.2, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 2;
    %cmp/u;
    %jmp/1 T_2.3, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 2;
    %cmp/u;
    %jmp/1 T_2.4, 6;
    %jmp T_2.5;
T_2.2 ;
    %pushi/vec4 0, 0, 3;
    %assign/vec4 v0x60644caa0c70_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x60644cac5440_0, 0;
    %load/vec4 v0x60644cac5250_0;
    %cmpi/e 0, 0, 1;
    %jmp/0xz  T_2.6, 4;
    %pushi/vec4 1, 0, 2;
    %assign/vec4 v0x60644cac5500_0, 0;
T_2.6 ;
    %jmp T_2.5;
T_2.3 ;
    %load/vec4 v0x60644cac5250_0;
    %cmpi/e 1, 0, 1;
    %jmp/0xz  T_2.8, 4;
    %pushi/vec4 0, 0, 2;
    %assign/vec4 v0x60644cac5500_0, 0;
    %jmp T_2.9;
T_2.8 ;
    %load/vec4 v0x60644cac5960_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_2.10, 8;
    %load/vec4 v0x60644cac5a20_0;
    %parti/s 7, 0, 2;
    %load/vec4 v0x60644cac55e0_0;
    %concat/vec4; draw_concat_vec4
    %assign/vec4 v0x60644cac5a20_0, 0;
    %load/vec4 v0x60644caa0c70_0;
    %addi 1, 0, 3;
    %assign/vec4 v0x60644caa0c70_0, 0;
    %load/vec4 v0x60644caa0c70_0;
    %cmpi/e 7, 0, 3;
    %jmp/0xz  T_2.12, 4;
    %pushi/vec4 2, 0, 2;
    %assign/vec4 v0x60644cac5500_0, 0;
T_2.12 ;
T_2.10 ;
T_2.9 ;
    %jmp T_2.5;
T_2.4 ;
    %load/vec4 v0x60644cac5a20_0;
    %assign/vec4 v0x60644cac5310_0, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x60644cac5440_0, 0;
    %pushi/vec4 0, 0, 2;
    %assign/vec4 v0x60644cac5500_0, 0;
    %jmp T_2.5;
T_2.5 ;
    %pop/vec4 1;
T_2.1 ;
    %jmp T_2;
    .thread T_2;
    .scope S_0x60644ca98870;
T_3 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x60644cac5f70_0, 0, 1;
T_3.0 ;
    %delay 20000, 0;
    %load/vec4 v0x60644cac5f70_0;
    %inv;
    %store/vec4 v0x60644cac5f70_0, 0, 1;
    %jmp T_3.0;
    %end;
    .thread T_3;
    .scope S_0x60644ca98870;
T_4 ;
    %vpi_call/w 3 39 "$dumpfile", "waveform.vcd" {0 0 0};
    %vpi_call/w 3 40 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x60644ca98870 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x60644cac6370_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x60644cac6080_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x60644cac6410_0, 0, 1;
